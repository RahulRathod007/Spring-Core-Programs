<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
		<beans>
			<bean id="parcel" class="org.techhub.Parcel">
				<property name="id" value="1"/>
				<property name="name" value="TV"/>
				<property name="SourceAddress" value="Pune"/>
				<property name="DestAddress" value="Nashik"/>
			</bean>
			
			<bean id="c" class="org.techhub.Courier" autowire="autodetect">
				<property name="CompanyName" value="ABC Company"/>
				<property name="Website" value="https://ABC.com"/>
			</bean>
		</beans>