<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
	<beans>
		<!-- Adding Fist Employee -->
		<bean id="e" class="Org.Techhub.Employee">
		<property name="id" value="1"/>
		<property name="name" value="Rahul"/>
		<property name="sal" value="1000"/>
		</bean>
		
		<!--  Adding Second Employee  -->
		<bean id="em" class="Org.Techhub.Employee">
		<property name="id" value="2"/>
		<property name="name" value="Sakshi"/>
		<property name="sal" value="2000"/>
		</bean>
	</beans>